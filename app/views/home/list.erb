<div class="container" style="margin-top:50px;">
    <H1>글 모음</H1>
    <% @every_post.each do |p|%>
    <h3>제목:<%=p.title%><br></h3>
    <h3>내용:<%=p.content%></h3>
    <h3>시간:<%=p.created_at%></h3>
    <h4><a href="/destroy/<%=p.id%>">[삭제]</a>
    <a href="/update_view/<%=p.id%>">[수정]</a></h4>
    <hr>
    <%end%>
    <h4><a href="/">글 추가</a></h4>
</div>