<div class="container" style="margin-top:50px;">
    <h1>수정</h1>
    <form action="/renew/<%=@one_post.id%>" method="POST">
      <div class="form-group">
        <label for="exampleInputEmail1">제목</label>
        <input name="title" type="text" class="form-control" id="exampleInputEmail1" value="<%=@one_post.title%>">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1">내용</label>
        <textarea name="content" class="form-control" rows="5" id="comment"><%=@one_post.content%></textarea>
    <!--    <input type="text" class="form-control" id="exampleInputPassword1" placeholder="내용을 입력해 주세요.">-->
      </div>
      <button type="submit" class="btn btn-default">수정</button>
    </form>
</div>